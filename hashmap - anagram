def anagrams(s1, s2):
  # this is the main function
  return (create_dict(s1) == create_dict(s2))
  
  
# this is a helper function
def create_dict(s):
  count = {}
  for char in s:
    if char not in count:
      count[char] = 0
    else: #char is in count
      count[char] = count[char] + 1
  return count
  
  
  # again?
  class Solution:
    def containsDuplicate(self, nums: List[int]) -> bool:
        hashset = set()
        # check if it's in all nums
        for num in nums:
            if num in hashset:
                return True
            else:
                hashset.add(num)

        return False

        
